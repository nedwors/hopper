{
    "name": "nedwors/hopper",
    "description": "",
    "keywords": [
        "nedwors",
        "hopper"
    ],
    "homepage": "https://github.com/nedwors/hopper",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Sam Rowden",
            "email": "nedwors@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Nedwors\\Hopper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nedwors\\Hopper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nedwors\\Hopper\\HopperServiceProvider"
            ],
            "aliases": {
                "Hopper": "Nedwors\\Hopper\\HopperFacade"
            }
        }
    }
}
